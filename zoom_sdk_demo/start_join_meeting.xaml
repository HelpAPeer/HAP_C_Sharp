<Window x:Class="zoom_sdk_demo.start_join_meeting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zoom_sdk_demo"
        mc:Ignorable="d"
        Title="start_join_meeting" Height="348" Width="564"
        Closing="Wnd_Closing">
    <Grid Height="348" Margin="0,0,0,-78" HorizontalAlignment="Left" Width="0">
        <Label x:Name="label_UserName" Content="UserName" HorizontalAlignment="Left" Margin="32,21,-132,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="label_UserName_Copy" Content="UserID(start meeting)" HorizontalAlignment="Left" Margin="32,66,-170,0" VerticalAlignment="Top" Width="138"/>
        <Label x:Name="label_UserName_Copy2" Content="MeetingNumber" HorizontalAlignment="Left" Margin="32,159,-145,0" VerticalAlignment="Top" Width="113"/>
        <TextBox x:Name="textBox_username_api" HorizontalAlignment="Left" Height="26" Margin="191,21,-364,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="173"/>
        <TextBox x:Name="textBox_userid_api" HorizontalAlignment="Left" Height="26" Margin="191,68,-507,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="316"/>
        <TextBox x:Name="textBox_meetingnumber_api" HorizontalAlignment="Left" Height="26" Margin="191,159,-364,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="173"/>
        <Button x:Name="button_start_api" Content="Start" HorizontalAlignment="Left" Height="44" Margin="57,203,-199,0" VerticalAlignment="Top" Width="142" Click="button_start_api_Click"/>
        <Button x:Name="button_join_api" Content="Join" HorizontalAlignment="Left" Height="44" Margin="317,203,-459,0" VerticalAlignment="Top" Width="142" Click="button_join_api_Click"/>
        <Label x:Name="label_AccessToken" Content="AccessToken" HorizontalAlignment="Left" Margin="32,115,-145,0" VerticalAlignment="Top" Width="113"/>
        <TextBox x:Name="textBox_AccessToken" HorizontalAlignment="Left" Height="26" Margin="191,115,-507,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="316"/>
    </Grid>
</Window>
